<?xml version='1.0' encoding='utf-8'?>
<device xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="CMSIS-SVD.xsd" schemaVersion="1.3">
  <name>Test_Example</name>
  <version>1.0</version>
  <description>Test_Example device</description>
  <addressUnitBits>8</addressUnitBits>
  <width>32</width>
  <size>32</size>
  <access>read-write</access>
  <resetValue>0x0</resetValue>
  <resetMask>0xffffffff</resetMask>
  <protection>s</protection>
  <peripherals>
    <peripheral derivedFrom="ADC1">
      <name>ADC2</name>
      <version>2.0</version>
      <description>Analog-to-Digital Converter2</description>
      <alternatePeripheral>ADCy</alternatePeripheral>
      <groupName>ADCc</groupName>
      <prependToName>ADC!</prependToName>
      <appendToName>!ADC</appendToName> 
      <headerStructName>abc</headerStructName>
      <disableCondition>$ADC1</disableCondition>
      <baseAddress>0x40001000</baseAddress>
      <addressBlock>
        <offset>0x1</offset>
        <size>0x2000</size>
        <usage>buffer</usage>
      </addressBlock>
      <interrupt>
        <name>ADC2</name>
        <value>19</value>
      </interrupt>
    </peripheral>
    <peripheral>
      <name>ADC1</name>
      <version>1.0</version>
      <description>Analog-to-Digital Converter</description>
      <alternatePeripheral>ADCx</alternatePeripheral>
      <groupName>ADC</groupName>
      <prependToName>ADC_</prependToName>
      <appendToName>_ADC</appendToName> 
      <headerStructName>adc</headerStructName>
      <disableCondition>!ADC1</disableCondition>
      <baseAddress>0x40002000</baseAddress>
      <addressBlock>
        <offset>0x0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>
      <interrupt>
        <name>ADC1</name>
        <value>18</value>
      </interrupt>
      <registers>
        <register>
          <name>ADC_ISR</name>
          <description>ADC interrupt and status register</description>
          <addressOffset>0x0</addressOffset>
          <size>32</size>
          <resetValue>0x0</resetValue>
          <resetMask>0xffffffff</resetMask>
          <fields>
            <field>
              <name>ADRDY</name>
              <bitOffset>0</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
              <enumeratedValues>
                <name>adc_enumerated_value</name>
                <enumeratedValue>
                  <name>B_0x0</name>
                  <description>ADC is not ready to start conversion</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>B_0x1</name>
                  <description>ADC is ready to start conversion</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
      </registers>
    </peripheral>
  </peripherals>
</device>
